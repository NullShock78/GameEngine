//BEGIN
#version 430 core

in  vec2 UV;
in  vec3 normal;
in  vec3 lightPos;
in  vec3 Pos;
in  vec3 cameraPos;
out vec4 outColor;
layout(location = 20) uniform sampler2D textureSampler;
//layout(location = 31) uniform sampler2D normalSampler;

void main()
{
	vec3 tex = texture(textureSampler, UV).rgb;
	vec3 lightVec = normalize(Pos - lightPos);
	vec3 lightVec2 = normalize(lightPos - Pos);
	vec3 diffuse =  tex * ((lightVec.x * normal.x) + (lightVec.y * normal.y) + (lightVec.z * normal.z)) * 1.0f;
	vec3 reflectVec = vec3(reflect(lightVec2, normal));
	vec3 camVec = vec3(normalize(Pos - cameraPos));
	vec3 specular =  vec3(1.0,1.0,1.0) * tex * ((camVec.x * reflectVec.x) + (camVec.y * reflectVec.y) + (camVec.z * reflectVec.z)) * 0.30f;//((reflectVec.x * camVec.x) + (reflectVec.y * camVec.y) + (reflectVec.z * camVec.z)) * 0.3f;
	//specular = clamp(specular, 0.0, 1.0);
	vec3 finalLight = vec3(0.30, 0.30, 0.30) + diffuse + specular;

	
	outColor = vec4(finalLight, 1.0f);

}



out vec3 normal;
out vec3 color;

//out vec4 color;

void main()
{
	vec4 vertPos = vec4(vPosition, 1.0);
	vec4 matPos = MAT *  vertPos;
	gl_Position = matPos;

	vec3 matPos3 = vec3(matPos.x, matPos.y, matPos.z);

	vec3 lVec = vec3(MAT * vec4(lPos,1.0f)) - matPos3;

	float dist = length((lPos - matPos3));

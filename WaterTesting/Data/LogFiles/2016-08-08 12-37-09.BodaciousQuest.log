	float light = ((vNorm.x * lVec.r) + (vNorm.y * lVec.g) + (vNorm.z*lVec.b)) * -1.0f* (dist/10.0f);

	color = vec3(tint.r * light, tint.g*light,  tint.b * light);
	normal = vNorm;
	
	
}
//END


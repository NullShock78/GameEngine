layout (location = 0)  in vec3 vPosition;
layout (location = 1)  in vec3 vNorm;
layout (location = 15) uniform vec3 tint;
layout (location = 16) uniform mat4 MAT;

layout(location = 22) uniform vec3 lPos;
//NORMALIZE!!!!

#version 430 core

layout (location = 0)  in vec3 vPosition;
layout (location = 1)  in vec3 vNorm;
layout (location = 15) uniform vec3 tint;
layout (location = 16) uniform mat4 MAT;

layout(location = 22) uniform vec3 lPos;
//NORMALIZE!!!!
//layout(location = 23) uniform vec3 lVec;
//out data
out vec3 normal;
out vec3 color;

//out vec4 color;

void main()
{
	vec4 vertPos = vec4(vPosition, 1.0);
	vec4 matPos = MAT *  vertPos;
	gl_Position = matPos;

	vec3 matPos3 = vec3(matPos.x, matPos.y, matPos.z);

	vec3 lVec = vec3(MAT * vec4(lPos,1.0f)) - matPos3;

	float dist = length((lPos - matPos3));
	//vec3 matLightPos = vec3(MAT * vec4(lVec, 1.0f));

	//lightPos;
	//DOT

	float light = ((vNorm.x * lVec.r) + (vNorm.y * lVec.g) + (vNorm.z*lVec.b)) * -1.0f* (dist/10.0f);

	color = vec3(tint.r * light, tint.g*light,  tint.b * light);
	normal = vNorm;
	
	
}
//END

